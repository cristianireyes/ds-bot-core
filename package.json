  },
  "homepage": "https://github.com/cristianireyes/ds-bot-core#readme",
  "author": {
    "name": "Cristian Reyes",
    "email": "cristianir.dev@gmail.com",
    "url": "https://cristianreyes.me"
  },
  "contributors": [],
  "license": "ISC",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
